{
    "family": "llmops-live",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::887579663132:role/ecsTaskExecutionRole",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "8192",
    "containerDefinitions": [
      {
        "name": "llmops-live-container",
        "image": "887579663132.dkr.ecr.eu-north-1.amazonaws.com/llmopsrepolive",
        "cpu": 1024,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp",
            "name": "llmops-live-container-8080-tcp",
            "appProtocol": "http"
          }
        ],
        "environment": [
          {
            "name": "ENV",
            "value": "production"
          }
        ],
        "secrets": [
          {
            "name": "apikeyliveclass",
            "valueFrom": "arn:aws:secretsmanager:eu-north-1:887579663132:secret:apikeyliveclass-MCPEBV"
          }
  
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/llmops-live",
            "awslogs-region": "eu-north-1",
            "awslogs-stream-prefix": "ecs",
            "awslogs-create-group": "true"
          }
        }
      }
    ]
  }
  